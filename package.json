{
  "name": "gamtaneri",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "orm:build": "tsc --build  src",
    "migrations:generate": "typeorm migration:generate  -d src/dist/config/ormConfig.js",
    "migration:create": "typeorm migration:create src/migrations/user",
    "migration:run": "npm run orm:build && typeorm migration:run -d src/dist/config/ormConfig.js",
    "copy-files": "copyfiles -u 1 src/migrations/*.js dist/migrations"
  },
  "dependencies": {
    "mysql2": "^2.3.3",
    "next": "12.3.0",
    "nuka-carousel": "^5.3.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scroll": "^1.8.7",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.10"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@types/react": "18.0.20",
    "@types/react-dom": "18.0.6",
    "eslint": "8.23.1",
    "eslint-config-next": "12.3.0",
    "typescript": "4.8.3"
  }
}
